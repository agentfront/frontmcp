{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": ".",
    "rootDir": ".",
    "strict": true,
    "sourceMap": true,
    "inlineSources": true,
    "noImplicitAny": false,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "strictPropertyInitialization": false,
    "esModuleInterop": true,
    "importHelpers": true,
    "target": "es2022",
    "module": "esnext",
    "lib": ["es2022", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "typeRoots": ["./node_modules/@types", "./types"],
    "paths": {
      "@frontmcp/utils": ["libs/utils/src/index.ts"],
      "@frontmcp/utils/crypto/node": ["libs/utils/src/crypto/node.ts"],
      "@frontmcp/di": ["libs/di/src/index.ts"],
      "@frontmcp/adapters": ["libs/adapters/src/index.ts"],
      "@frontmcp/adapters/openapi": ["libs/adapters/src/openapi/index.ts"],
      "@frontmcp/plugins": ["libs/plugins/src/index.ts"],
      "@frontmcp/plugin-approval": ["plugins/plugin-approval/src/index.ts"],
      "@frontmcp/plugin-cache": ["plugins/plugin-cache/src/index.ts"],
      "@frontmcp/plugin-codecall": ["plugins/plugin-codecall/src/index.ts"],
      "@frontmcp/plugin-dashboard": ["plugins/plugin-dashboard/src/index.ts"],
      "@frontmcp/plugin-remember": ["plugins/plugin-remember/src/index.ts"],
      "@frontmcp/sdk": ["libs/sdk/src/index.ts"],
      "@frontmcp/testing": ["libs/testing/src/index.ts"],
      "@frontmcp/uipack": ["libs/uipack/src/index.ts"],
      "@frontmcp/uipack/types": ["libs/uipack/src/types/index.ts"],
      "@frontmcp/uipack/adapters": ["libs/uipack/src/adapters/index.ts"],
      "@frontmcp/uipack/base-template": ["libs/uipack/src/base-template/index.ts"],
      "@frontmcp/uipack/build": ["libs/uipack/src/build/index.ts"],
      "@frontmcp/uipack/renderers": ["libs/uipack/src/renderers/index.ts"],
      "@frontmcp/uipack/runtime": ["libs/uipack/src/runtime/index.ts"],
      "@frontmcp/uipack/bridge-runtime": ["libs/uipack/src/bridge-runtime/index.ts"],
      "@frontmcp/uipack/registry": ["libs/uipack/src/registry/index.ts"],
      "@frontmcp/uipack/dependency": ["libs/uipack/src/dependency/index.ts"],
      "@frontmcp/uipack/handlebars": ["libs/uipack/src/handlebars/index.ts"],
      "@frontmcp/uipack/typings": ["libs/uipack/src/typings/index.ts"],
      "@frontmcp/uipack/bundler": ["libs/uipack/src/bundler/index.ts"],
      "@frontmcp/uipack/theme": ["libs/uipack/src/theme/index.ts"],
      "@frontmcp/uipack/styles": ["libs/uipack/src/styles/index.ts"],
      "@frontmcp/uipack/tool-template": ["libs/uipack/src/tool-template/index.ts"],
      "@frontmcp/uipack/utils": ["libs/uipack/src/utils/index.ts"],
      "@frontmcp/uipack/validation": ["libs/uipack/src/validation/index.ts"],
      "@frontmcp/uipack/shell": ["libs/uipack/src/shell/index.ts"],
      "@frontmcp/uipack/component": ["libs/uipack/src/component/index.ts"],
      "@frontmcp/uipack/resolver": ["libs/uipack/src/resolver/index.ts"],
      "@frontmcp/ui": ["libs/ui/src/index.ts"],
      "@frontmcp/ui/components": ["libs/ui/src/components/index.ts"],
      "@frontmcp/ui/layouts": ["libs/ui/src/layouts/index.ts"],
      "@frontmcp/ui/pages": ["libs/ui/src/pages/index.ts"],
      "@frontmcp/ui/widgets": ["libs/ui/src/widgets/index.ts"],
      "@frontmcp/ui/bridge": ["libs/ui/src/bridge/index.ts"],
