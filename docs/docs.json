{
  "$schema": "https://mintlify.com/docs.json",
  "name": "FrontMCP",
  "description": "A TypeScript-first framework for building production ready Model Context Protocol (MCP) servers",
  "appearance": {
    "default": "system",
    "strict": false
  },
  "theme": "almond",
  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "contextual": {
    "options": [
      "copy",
      "chatgpt",
      "cursor",
      "mcp",
      "view"
    ]
  },
  "interaction": {
    "drilldown": true
  },
  "navbar": {
    "links": [
      {
        "href": "https://github.com/agentfront/frontmcp-examples",
        "icon": "github",
        "label": "Examples"
      }
    ],
    "primary": {
      "href": "https://github.com/agentfront/frontmcp",
      "type": "github"
    }
  },
  "search": {
    "prompt": "Search the FrontMCP docs..."
  },
  "styling": {
    "codeblocks": {
      "theme": {
        "dark": "dark-plus",
        "light": "snazzy-light"
      }
    }
  },
  "errors": {
    "404": {
      "redirect": true
    }
  },
