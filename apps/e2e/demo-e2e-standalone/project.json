{
  "name": "demo-e2e-standalone",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/e2e/demo-e2e-standalone/src",
  "projectType": "application",
  "tags": ["scope:demo", "type:e2e", "feature:standalone"],
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "development",
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/apps/e2e/demo-e2e-standalone",
        "main": "apps/e2e/demo-e2e-standalone/src/main.ts",
        "tsConfig": "apps/e2e/demo-e2e-standalone/tsconfig.app.json",
        "webpackConfig": "apps/e2e/demo-e2e-standalone/webpack.config.js",
        "generatePackageJson": true
      },
      "configurations": {
        "development": {},
        "production": {
          "optimization": true
        }
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "command": "node dist/apps/e2e/demo-e2e-standalone/main.js",
        "cwd": "{workspaceRoot}"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/apps/e2e/demo-e2e-standalone"],
      "options": {
        "jestConfig": "apps/e2e/demo-e2e-standalone/jest.e2e.config.ts",
        "passWithNoTests": true
      }
    }
  }
}
