{
  "name": "@frontmcp/sdk",
  "version": "0.12.1",
  "description": "FrontMCP SDK",
  "author": "AgentFront <info@agentfront.dev>",
  "homepage": "https://docs.agentfront.dev",
  "license": "Apache-2.0",
  "keywords": [
    "mcp",
    "dependency injection",
    "sdk",
    "adapters",
    "plugins",
    "agentfront",
    "frontmcp",
    "framework",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agentfront/frontmcp.git"
  },
  "bugs": {
    "url": "https://github.com/agentfront/frontmcp/issues"
  },
  "type": "commonjs",
  "main": "./dist/index.js",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/esm/index.mjs"
      }
    },
    "./transport": {
      "development": "./src/transport/index.ts",
      "require": {
        "types": "./dist/transport/index.d.ts",
        "default": "./dist/transport/index.js"
      },
      "import": {
        "types": "./dist/transport/index.d.ts",
        "default": "./dist/esm/transport/index.mjs"
      }
    },
    "./esm": null
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "peerDependencies": {
    "zod": "^4.0.0",
    "zod-from-json-schema": "^0.5.2",
    "express": "^4.18.0 || ^5.0.0",
    "cors": "^2.8.5",
    "raw-body": "^3.0.0",
    "content-type": "^1.0.5",
    "vectoriadb": "^2.1.3",
    "@vercel/kv": "^3.0.0",
    "@frontmcp/storage-sqlite": "0.12.1",
    "@enclave-vm/core": "^2.11.1"
  },
  "peerDependenciesMeta": {
    "@vercel/kv": {
      "optional": true
    },
    "@frontmcp/storage-sqlite": {
      "optional": true
    },
    "@enclave-vm/core": {
      "optional": true
    }
  },
  "dependencies": {
    "@frontmcp/utils": "0.12.1",
    "@frontmcp/di": "0.12.1",
    "@frontmcp/uipack": "0.12.1",
    "@frontmcp/auth": "0.12.1",
    "@modelcontextprotocol/sdk": "1.26.0",
    "@langchain/core": "^1.1.8",
    "@langchain/openai": "^1.2.0",
    "@langchain/anthropic": "^1.3.3",
    "ioredis": "^5.8.0",
    "js-yaml": "^4.1.1",
    "jose": "^6.1.3",
    "reflect-metadata": "^0.2.2"
  },
  "optionalDependencies": {
    "@langchain/google-genai": "^2.1.3",
    "@langchain/mistralai": "^1.0.2",
    "@langchain/groq": "^1.0.2"
  },
  "devDependencies": {
    "typescript": "^5.9.3"
  }
}
