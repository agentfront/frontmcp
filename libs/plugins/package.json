{
  "name": "@frontmcp/plugins",
  "version": "0.6.3",
  "description": "FrontMCP plugins to extend the SDK",
  "author": "AgentFront <info@agentfront.dev>",
  "homepage": "https://docs.agentfront.dev",
  "license": "Apache-2.0",
  "keywords": [
    "mcp",
    "plugins",
    "agentfront",
    "frontmcp",
    "framework",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agentfront/frontmcp.git",
    "directory": "libs/plugins"
  },
  "bugs": {
    "url": "https://github.com/agentfront/frontmcp/issues"
  },
  "type": "commonjs",
  "main": "./dist/index.js",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/esm/index.mjs"
      }
    },
    "./cache": {
      "development": "./src/cache/index.ts",
      "require": {
        "types": "./dist/cache/index.d.ts",
        "default": "./dist/cache/index.js"
      },
      "import": {
        "types": "./dist/cache/index.d.ts",
        "default": "./dist/esm/cache/index.mjs"
      }
    },
    "./codecall": {
      "development": "./src/codecall/index.ts",
      "require": {
        "types": "./dist/codecall/index.d.ts",
        "default": "./dist/codecall/index.js"
      },
      "import": {
        "types": "./dist/codecall/index.d.ts",
        "default": "./dist/esm/codecall/index.mjs"
      }
    },
    "./esm": null
  },
  "dependencies": {
    "ioredis": "^5.8.0",
    "@frontmcp/sdk": "0.6.3",
    "enclave-vm": "^1.0.3",
    "vectoriadb": "^2.0.1",
    "zod": "^4.0.0",
    "@modelcontextprotocol/sdk": "1.25.1"
  },
  "devDependencies": {
    "reflect-metadata": "^0.2.2"
  },
  "peerDependencies": {
    "@vercel/kv": "^2.0.0 || ^3.0.0"
  },
  "peerDependenciesMeta": {
    "@vercel/kv": {
      "optional": true
    }
  }
}
