{
  "name": "@frontmcp/vectoria",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/vectoria/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "libs/vectoria/dist",
        "main": "libs/vectoria/src/index.ts",
        "tsConfig": "libs/vectoria/tsconfig.lib.json",
        "assets": ["libs/vectoria/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/vectoria/jest.config.ts"
      }
    }
  },
  "tags": []
}
