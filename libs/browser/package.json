{
  "name": "@frontmcp/browser",
  "version": "0.6.0",
  "description": "Browser-native MCP server implementation for @frontmcp/sdk",
  "author": "AgentFront <info@agentfront.dev>",
  "homepage": "https://docs.agentfront.dev",
  "license": "Apache-2.0",
  "keywords": [
    "mcp",
    "browser",
    "model-context-protocol",
    "valtio",
    "react",
    "web-crypto",
    "agentfront",
    "frontmcp",
    "framework",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agentfront/frontmcp.git",
    "directory": "libs/browser"
  },
  "bugs": {
    "url": "https://github.com/agentfront/frontmcp/issues"
  },
  "type": "commonjs",
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./react": {
      "development": "./src/react/index.ts",
      "require": {
        "types": "./dist/react/index.d.ts",
        "default": "./dist/react/index.js"
      },
      "import": {
        "types": "./dist/esm/react/index.d.ts",
        "default": "./dist/esm/react/index.js"
      }
    },
    "./store": {
      "development": "./src/store/index.ts",
      "require": {
        "types": "./dist/store/index.d.ts",
        "default": "./dist/store/index.js"
      },
      "import": {
        "types": "./dist/esm/store/index.d.ts",
        "default": "./dist/esm/store/index.js"
      }
    },
    "./telemetry": {
      "development": "./src/telemetry/index.ts",
      "require": {
        "types": "./dist/telemetry/index.d.ts",
        "default": "./dist/telemetry/index.js"
      },
      "import": {
        "types": "./dist/esm/telemetry/index.d.ts",
        "default": "./dist/esm/telemetry/index.js"
      }
    },
    "./a11y": {
      "development": "./src/a11y/index.ts",
      "require": {
        "types": "./dist/a11y/index.d.ts",
        "default": "./dist/a11y/index.js"
      },
      "import": {
        "types": "./dist/esm/a11y/index.d.ts",
        "default": "./dist/esm/a11y/index.js"
      }
    },
    "./plugins": {
      "development": "./src/plugins/index.ts",
      "require": {
        "types": "./dist/plugins/index.d.ts",
        "default": "./dist/plugins/index.js"
      },
      "import": {
        "types": "./dist/esm/plugins/index.d.ts",
        "default": "./dist/esm/plugins/index.js"
      }
    },
    "./host": {
      "development": "./src/host/index.ts",
      "require": {
        "types": "./dist/host/index.d.ts",
        "default": "./dist/host/index.js"
      },
      "import": {
        "types": "./dist/esm/host/index.d.ts",
        "default": "./dist/esm/host/index.js"
      }
    },
    "./polyfill": {
      "development": "./src/polyfill/index.ts",
      "require": {
        "types": "./dist/polyfill/index.d.ts",
        "default": "./dist/polyfill/index.js"
      },
      "import": {
        "types": "./dist/esm/polyfill/index.d.ts",
        "default": "./dist/esm/polyfill/index.js"
      }
    },
    "./types": {
      "development": "./src/polyfill/types.ts",
      "require": {
        "types": "./dist/polyfill/types.d.ts",
        "default": "./dist/polyfill/types.js"
      },
      "import": {
        "types": "./dist/esm/polyfill/types.d.ts",
        "default": "./dist/esm/polyfill/types.js"
      }
    },
    "./components": {
      "development": "./src/components/index.ts",
      "require": {
        "types": "./dist/components/index.d.ts",
        "default": "./dist/components/index.js"
      },
      "import": {
        "types": "./dist/esm/components/index.d.ts",
        "default": "./dist/esm/components/index.js"
      }
    },
    "./theme": {
      "development": "./src/theme/index.ts",
      "require": {
        "types": "./dist/theme/index.d.ts",
        "default": "./dist/theme/index.js"
      },
      "import": {
        "types": "./dist/esm/theme/index.d.ts",
        "default": "./dist/esm/theme/index.js"
      }
    },
    "./esm": null
  },
  "sideEffects": [
    "./polyfill/index.js",
    "./esm/polyfill/index.js"
  ],
  "dependencies": {
    "@frontmcp/sdk": "0.6.0",
    "valtio": "^2.1.2",
    "idb": "^8.0.0"
  },
  "peerDependencies": {
    "zod": "^4.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "typescript": "^5.9.3"
  }
}
