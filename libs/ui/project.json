{
  "name": "ui",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/ui/src",
  "projectType": "library",
  "tags": ["scope:libs", "scope:publishable", "versioning:synchronized"],
  "targets": {
    "build-cjs": {
      "executor": "@nx/esbuild:esbuild",
      "dependsOn": ["^build"],
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "libs/ui/dist",
        "main": "libs/ui/src/index.ts",
        "tsConfig": "libs/ui/tsconfig.lib.json",
        "format": ["cjs"],
        "declaration": true,
        "declarationRootDir": "libs/ui/src",
        "bundle": true,
        "thirdParty": false,
        "platform": "neutral",
        "assets": ["libs/ui/README.md", "LICENSE", "libs/ui/package.json"],
        "additionalEntryPoints": [
          "libs/ui/src/bridge/index.ts",
          "libs/ui/src/theme/index.ts",
          "libs/ui/src/components/index.ts",
          "libs/ui/src/components/Button/index.ts",
          "libs/ui/src/components/Card/index.ts",
          "libs/ui/src/components/Alert/index.ts",
          "libs/ui/src/components/Badge/index.ts",
          "libs/ui/src/components/Avatar/index.ts",
          "libs/ui/src/components/Modal/index.ts",
          "libs/ui/src/components/Table/index.ts",
          "libs/ui/src/components/TextField/index.ts",
          "libs/ui/src/components/Select/index.ts",
          "libs/ui/src/components/List/index.ts",
          "libs/ui/src/components/Loader/index.ts",
          "libs/ui/src/renderer/index.ts",
          "libs/ui/src/renderer/mdx/index.ts",
          "libs/ui/src/renderer/html/index.ts",
          "libs/ui/src/renderer/react/index.ts",
          "libs/ui/src/renderer/pdf/index.ts",
          "libs/ui/src/renderer/csv/index.ts",
          "libs/ui/src/renderer/charts/index.ts",
          "libs/ui/src/renderer/mermaid/index.ts",
          "libs/ui/src/renderer/flow/index.ts",
          "libs/ui/src/renderer/math/index.ts",
          "libs/ui/src/renderer/maps/index.ts",
          "libs/ui/src/renderer/image/index.ts",
          "libs/ui/src/renderer/media/index.ts",
          "libs/ui/src/renderer/common/index.ts",
          "libs/ui/src/react/index.ts",
          "libs/ui/src/runtime/index.ts"
        ],
        "esbuildOptions": {
          "outExtension": { ".js": ".js" },
          "external": [
            "react",
            "react-dom",
            "react/*",
            "react-dom/*",
            "@mui/material",
            "@mui/material/*",
            "@emotion/react",
            "@emotion/react/*",
            "@emotion/styled",
            "@emotion/styled/*",
            "@frontmcp/uipack",
            "@frontmcp/uipack/*",
            "recharts",
            "recharts/*",
            "react-markdown",
            "remark-gfm",
            "rehype-highlight",
            "rehype-raw",
            "mermaid",
            "mermaid/*",
            "@xyflow/react",
            "@xyflow/react/*",
            "katex",
            "katex/*",
            "react-leaflet",
            "react-leaflet/*",
            "leaflet",
            "leaflet/*",
            "react-player",
            "react-player/*",
            "react-pdf",
            "react-pdf/*",
            "pdfjs-dist",
            "pdfjs-dist/*",
            "dompurify",
            "dompurify/*"
          ]
        }
      }
    },
    "build-esm": {
      "executor": "@nx/esbuild:esbuild",
      "dependsOn": ["build-cjs"],
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "libs/ui/dist/esm",
        "main": "libs/ui/src/index.ts",
        "tsConfig": "libs/ui/tsconfig.lib.json",
        "format": ["esm"],
        "declaration": false,
        "bundle": true,
        "thirdParty": false,
        "platform": "neutral",
        "additionalEntryPoints": [
          "libs/ui/src/bridge/index.ts",
          "libs/ui/src/theme/index.ts",
          "libs/ui/src/components/index.ts",
          "libs/ui/src/components/Button/index.ts",
          "libs/ui/src/components/Card/index.ts",
          "libs/ui/src/components/Alert/index.ts",
          "libs/ui/src/components/Badge/index.ts",
          "libs/ui/src/components/Avatar/index.ts",
          "libs/ui/src/components/Modal/index.ts",
          "libs/ui/src/components/Table/index.ts",
          "libs/ui/src/components/TextField/index.ts",
          "libs/ui/src/components/Select/index.ts",
          "libs/ui/src/components/List/index.ts",
          "libs/ui/src/components/Loader/index.ts",
          "libs/ui/src/renderer/index.ts",
          "libs/ui/src/renderer/mdx/index.ts",
          "libs/ui/src/renderer/html/index.ts",
          "libs/ui/src/renderer/react/index.ts",
          "libs/ui/src/renderer/pdf/index.ts",
          "libs/ui/src/renderer/csv/index.ts",
          "libs/ui/src/renderer/charts/index.ts",
          "libs/ui/src/renderer/mermaid/index.ts",
          "libs/ui/src/renderer/flow/index.ts",
          "libs/ui/src/renderer/math/index.ts",
          "libs/ui/src/renderer/maps/index.ts",
          "libs/ui/src/renderer/image/index.ts",
          "libs/ui/src/renderer/media/index.ts",
          "libs/ui/src/renderer/common/index.ts",
          "libs/ui/src/react/index.ts",
          "libs/ui/src/runtime/index.ts"
        ],
        "esbuildOptions": {
          "outExtension": { ".js": ".mjs" },
          "external": [
            "react",
            "react-dom",
            "react/*",
            "react-dom/*",
            "@mui/material",
            "@mui/material/*",
            "@emotion/react",
            "@emotion/react/*",
            "@emotion/styled",
            "@emotion/styled/*",
            "@frontmcp/uipack",
            "@frontmcp/uipack/*",
            "recharts",
            "recharts/*",
            "react-markdown",
            "remark-gfm",
            "rehype-highlight",
            "rehype-raw",
            "mermaid",
            "mermaid/*",
            "@xyflow/react",
            "@xyflow/react/*",
            "katex",
            "katex/*",
            "react-leaflet",
            "react-leaflet/*",
            "leaflet",
            "leaflet/*",
            "react-player",
            "react-player/*",
            "react-pdf",
            "react-pdf/*",
            "pdfjs-dist",
            "pdfjs-dist/*",
            "dompurify",
            "dompurify/*"
          ]
        }
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "dependsOn": ["build-cjs", "build-esm"],
      "options": {
        "command": "node scripts/strip-dist-from-pkg.js libs/ui/dist/package.json"
      }
    },
    "publish": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "command": "npm publish libs/ui/dist --access public --registry=https://registry.npmjs.org/"
      }
    },
    "publish-alpha": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "command": "bash scripts/publish-alpha.sh {projectRoot}/dist",
        "cwd": "{workspaceRoot}"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/ui"],
      "options": {
        "jestConfig": "libs/ui/jest.config.ts",
        "passWithNoTests": true
      }
    }
  }
}
