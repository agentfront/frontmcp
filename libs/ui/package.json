{
  "name": "@frontmcp/ui",
  "version": "0.4.0",
  "description": "FrontMCP UI - Comprehensive UI library for authentication and authorization flows with multi-framework support (HTML, React, MDX)",
  "author": "AgentFront <info@agentfront.dev>",
  "homepage": "https://docs.agentfront.dev",
  "license": "Apache-2.0",
  "keywords": [
    "mcp",
    "ui",
    "tailwind",
    "htmx",
    "oauth",
    "authentication",
    "authorization",
    "consent",
    "agentfront",
    "frontmcp",
    "framework",
    "typescript",
    "react",
    "mdx",
    "jsx",
    "ssr"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agentfront/frontmcp.git"
  },
  "bugs": {
    "url": "https://github.com/agentfront/frontmcp/issues"
  },
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js",
      "default": "./dist/src/index.js"
    },
    "./renderers": {
      "development": "./src/renderers/index.ts",
      "types": "./dist/src/renderers/index.d.ts",
      "import": "./dist/src/renderers/index.js",
      "default": "./dist/src/renderers/index.js"
    }
  },
  "dependencies": {
    "@swc/core": "^1.5.0",
    "tslib": "^2.3.0",
    "zod": "^4.0.0"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "optionalDependencies": {
    "@mdx-js/mdx": "^3.1.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "typescript": "^5.9.3"
  }
}
