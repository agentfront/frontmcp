{
  "name": "@frontmcp/ui",
  "version": "0.12.0-rc.1",
  "description": "FrontMCP UI - React components and hooks for MCP applications",
  "author": "AgentFront <info@agentfront.dev>",
  "homepage": "https://docs.agentfront.dev",
  "license": "Apache-2.0",
  "keywords": [
    "mcp",
    "ui",
    "agentfront",
    "frontmcp",
    "react",
    "hooks",
    "components",
    "ssr",
    "jsx"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agentfront/frontmcp.git"
  },
  "bugs": {
    "url": "https://github.com/agentfront/frontmcp/issues"
  },
  "type": "commonjs",
  "main": "./dist/index.js",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/esm/index.mjs"
      }
    },
    "./*": {
      "development": "./src/*/index.ts",
      "require": {
        "types": "./dist/*/index.d.ts",
        "default": "./dist/*/index.js"
      },
      "import": {
        "types": "./dist/*/index.d.ts",
        "default": "./dist/esm/*/index.mjs"
      }
    },
    "./esm": null
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "dependencies": {
    "@mdx-js/mdx": "^3.1.1",
    "@swc/core": "^1.5.0",
    "esbuild": "^0.27.3",
    "zod": "^4.0.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "@frontmcp/uipack": "0.12.0-rc.1"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "typescript": "^5.9.3"
  }
}
