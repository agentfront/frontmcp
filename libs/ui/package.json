{
  "name": "@frontmcp/ui",
  "version": "0.5.1",
  "description": "FrontMCP UI - Comprehensive UI library for authentication and authorization flows with multi-framework support (HTML, React, MDX)",
  "author": "AgentFront <info@agentfront.dev>",
  "homepage": "https://docs.agentfront.dev",
  "license": "Apache-2.0",
  "keywords": [
    "mcp",
    "ui",
    "tailwind",
    "htmx",
    "oauth",
    "authentication",
    "authorization",
    "consent",
    "agentfront",
    "frontmcp",
    "framework",
    "typescript",
    "react",
    "mdx",
    "jsx",
    "ssr"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agentfront/frontmcp.git"
  },
  "bugs": {
    "url": "https://github.com/agentfront/frontmcp/issues"
  },
  "type": "commonjs",
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./*": {
      "development": "./src/*/index.ts",
      "require": {
        "types": "./dist/*/index.d.ts",
        "default": "./dist/*/index.js"
      },
      "import": {
        "types": "./dist/esm/*/index.d.ts",
        "default": "./dist/esm/*/index.js"
      }
    },
    "./esm": null
  },
  "dependencies": {
    "enclave-vm": "^1.0.3",
    "esbuild": "^0.27.1",
    "handlebars": "^4.7.8",
    "zod": "^4.0.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "optionalDependencies": {
    "@mdx-js/mdx": "^3.1.0",
    "@swc/core": "^1.5.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "typescript": "^5.9.3"
  }
}
