{
  "name": "@frontmcp/ui-shells",
  "version": "0.1.0",
  "description": "FrontMCP UI Shells - Server-side HTML shell templates for MCP tool authors",
  "author": "AgentFront <info@agentfront.dev>",
  "homepage": "https://docs.agentfront.dev",
  "license": "Apache-2.0",
  "keywords": [
    "mcp",
    "ui",
    "shells",
    "templates",
    "agentfront",
    "frontmcp",
    "html",
    "server-side"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agentfront/frontmcp.git"
  },
  "bugs": {
    "url": "https://github.com/agentfront/frontmcp/issues"
  },
  "type": "commonjs",
  "main": "./dist/index.js",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/esm/index.mjs"
      }
    },
    "./*": {
      "development": "./src/*/index.ts",
      "require": {
        "types": "./dist/*/index.d.ts",
        "default": "./dist/*/index.js"
      },
      "import": {
        "types": "./dist/*/index.d.ts",
        "default": "./dist/esm/*/index.mjs"
      }
    },
    "./esm": null
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "dependencies": {
    "@frontmcp/uipack": "*"
  },
  "devDependencies": {
    "typescript": "^5.9.3"
  }
}
