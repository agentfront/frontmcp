{
  "name": "@frontmcp/plugin-store",
  "version": "0.9.0",
  "description": "Reactive state store plugin for FrontMCP - Valtio-backed stores accessible via MCP resources",
  "author": "AgentFront <info@agentfront.dev>",
  "license": "Apache-2.0",
  "keywords": [
    "mcp",
    "store",
    "state",
    "valtio",
    "reactive",
    "plugin",
    "frontmcp",
    "agentfront"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agentfront/frontmcp.git",
    "directory": "plugins/plugin-store"
  },
  "bugs": {
    "url": "https://github.com/agentfront/frontmcp/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "type": "commonjs",
  "main": "./dist/index.js",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "browser": {
        "types": "./dist/index.d.ts",
        "import": "./dist/browser/browser.mjs",
        "default": "./dist/browser/browser.js"
      },
      "development": "./src/index.ts",
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/esm/index.mjs"
      }
    },
    "./browser": {
      "types": "./dist/index.d.ts",
      "import": "./dist/browser/browser.mjs",
      "default": "./dist/browser/browser.js"
    }
  },
  "dependencies": {
    "valtio": "^2.3.0",
    "@frontmcp/sdk": "0.9.0",
    "@modelcontextprotocol/sdk": "^1.26.0"
  },
  "devDependencies": {
    "reflect-metadata": "^0.2.2"
  }
}
